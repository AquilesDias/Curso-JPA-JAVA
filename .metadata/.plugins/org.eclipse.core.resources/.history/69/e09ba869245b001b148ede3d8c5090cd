package modelo.umpramuitos;

import javax.persistence.ManyToOne;

import modelo.basico.Produto;

public class ItemPedido {
	
	private Long id;
	
	private int quantidade;
	
	private double preco;

	@ManyToOne
	private Pedido pedido;
	
	@ManyToOne
	private Produto produto;
	
	public ItemPedido() {}
	

}
